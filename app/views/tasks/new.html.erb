<h1>Task#new</h1>
<p>Find me in app/views/task/new.html.erb</p>
<h1>Adding tasks to (SPECIFY LIST)</h1>

<table>
  <tr>
    <th>Task</th>
    <th>Date Created</th>
    <th></th>
  </tr>

<% @tasks.each do |task| %>
  <tr>
    <td><%= task.name %></td>
    <td><%= task.created_at.strftime("%D") %></td>
  </tr>
<% end %>
</table>

